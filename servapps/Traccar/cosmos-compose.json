{
    "cosmos-installer": {

      "post-install": [
        {
          "type": "info",
          "label": "App for tracking a various of gps."
        }
      ]
    },
    "services": {
      "{ServiceName}": {
        "image": "tananaev/traccar:latest",
        "container_name": "{ServiceName}",
        "hostname": "{ServiceName}",
        "restart": "unless-stopped",
        "UID": 1000,
        "GID": 1000,
        "ports": [
          "5000-5150:5000-5150/tcp",
          "8082:8082/tcp"
        ],
        "labels": {
          "cosmos-auto-update": "true",
          "cosmos-icon": "https://azukaar.github.io/cosmos-servapps-official/servapps/Traccar/icon.png"
        },
        "volumes": [
          {
            "source": "{ServiceName}.xml",
            "target": "/opt/traccar/traccar.xml",
            "type": "volume"
          }
        ],
        "routes": [
          {
            "name": "{ServiceName}",
            "description": "Expose {ServiceName} to the web",
            "useHost": true,
            "target": "http://{ServiceName}:8082",
            "mode": "SERVAPP",
            "Timeout": 14400000,
            "ThrottlePerMinute": 12000,
            "BlockCommonBots": true,
            "SmartShield": {
              "Enabled": true
            }
          }
        ],
        "networks": {
          "{ServiceName}-mysql": {}
        }
      }
      ,"{ServiceName}-mysql": {
        "image": "mysql:5.6.30",
        "container_name": "{ServiceName}-mysql",
        "hostname": "{ServiceName}-mysql",
        "restart": "unless-stopped",
        "UID": 1000,
        "GID": 1000,
        "environment": [
          "MYSQL_DATABASE=traccar",
          "MYSQL_ROOT_PASSWORD=traccar",
          "MYSQL_USER=traccar",
          "MYSQL_PASS=traccar"
        ],
        "labels": {
          "cosmos-auto-update": "true"
        },
        "volumes": [
          {
            "source": "{ServiceName}-mysql",
            "target": "/var/lib/mysql",
            "type": "volume"
          }
        ],
        "networks": {
          "{ServiceName}-mysql": {}
        }


      }
    }
  }
  